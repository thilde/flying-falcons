
function Four_Leaf_Clover() {
  flyCurve(60, 104, 0, 0, 230, 0, "in");
  flyCurve(-60, -104, 0, 0, -230, 0, "in");
  flyCurve(60, -104, 0, 0, -230, 0, "in");
  flyCurve(-60, 104, 0, 0, 230, 0, "in");
  flyCurve(104, 60, 0, 230, 0, 0, "in");
  flyCurve(-104, -60, 0, -230, 0, 0, "in");
  flyCurve(-104, -60, 0, -230, 0, 0, "in");
  flyCurve(104, 60, 0, 230, 0, 0, "in");
}


takeOff();
Four_Leaf_Clover();
land();
